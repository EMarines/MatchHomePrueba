<%- include("template/cabecera", {tituloWeb: 'PÃ¡gina de Inicio'}) %>


  <div class="container">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Nombre</th>
          <th scope="col">Email</th>
          <th scope="col">Accion</th>
        </tr>
      </thead>
      <tbody>

        <% if (arrayContactos.length > 0) { %>
          <% arrayContactos.forEach(contacto => { %>
            <tr>
              <th scope="row"><%= contacto.id %> </th>
              <td><%= contacto.name %> </td>
              <td><%= contacto.email %> </td>
              <td>@mdo</td>
            </tr>
    
          <% }) %>         
        <% } %>

      </tbody>
    </table>

  </div>

